    "argon2": "^0.43.0",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "googleapis": "^149.0.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "next": "15.3.1",
    "nodemailer": "^7.0.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
